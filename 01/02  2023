int main() {
	int weeknum, monthnum;
	std::cout << "Enter number of day 1-7 : " << endl;
	cin >> weeknum;
	switch (weeknum)
	{
	case 1:
		std::cout << "Monday" << endl; break;
	case 2:
		cout << "Tuesday" << endl;
	case 3:
		cout << "Wednesday" << endl;
	case 4:
		cout << "Thirsday" << endl;
	case 5:
		cout << "Friday" << endl;
	case 6:
		cout << "Saturday" << endl;
	case 7:
		cout << "Sunday" << endl;
	default:
		cout << "nav";
		break;
	}

	cout << "Enter number of month 1-12 : " << endl;
}
